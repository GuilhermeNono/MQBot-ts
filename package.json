{
  "name": "mqbot-ts",
  "version": "4.0.0",
  "description": "Uma portabilidade do bot de javascript para typescript",
  "main": "index.js",
  "engines": {
    "node": "16.x",
    "npm": "8.3.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/index.js",
    "postinstall": "npm run build",
    "dev": "nodemon --watch src/ --exec ts-node -r tsconfig-paths/register src/index.ts -e ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuilhermeNono/MQBot-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GuilhermeNono/MQBot-ts/issues"
  },
  "homepage": "https://github.com/GuilhermeNono/MQBot-ts#readme",
  "dependencies": {
    "@typegoose/typegoose": "^9.4.0",
    "@types/node": "^16.10.9",
    "canvas": "^2.8.0",
    "colors": "^1.4.0",
    "discord.js": "^13.5.1",
    "discordjs-button-pagination": "^1.0.7",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "mongoose": "^6.1.6",
    "ms": "^2.1.3",
    "nodemon": "^2.0.13",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31"
  }
}
